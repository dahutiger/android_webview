<?xml version="1.0" encoding="utf-8"?><resources>        <style name="Button" parent="Widget.AppCompat.Button.Borderless">        <item name="android:background">@null</item>    </style>        <style name="Button.Title">        <item name="android:textSize">@dimen/font_14</item>        <item name="android:textColor">@color/colorPrimary</item>    </style>        <style name="Button.Primary">        <item name="android:background">@drawable/btn_primary</item>        <item name="android:textColor">@color/white</item>        <item name="android:textSize">@dimen/font_16</item>    </style>        <style name="Divider">        <item name="android:background">@color/divider</item>    </style>        <style name="Divider.Horizontal">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">0.5dp</item>    </style>        <style name="Divider.Vertical">        <item name="android:layout_width">0.5dp</item>        <item name="android:layout_height">match_parent</item>    </style>        <style name="DialogButton">        <item name="android:layout_width">0dp</item>        <item name="android:layout_height">match_parent</item>        <item name="android:layout_weight">1</item>        <item name="android:background">@null</item>        <item name="android:gravity">center</item>        <item name="android:maxLines">2</item>        <item name="android:minHeight">@dimen/dialog_button_min_height</item>        <item name="android:textSize">@dimen/font_16</item>    </style>        <style name="DialogButton.Left">        <item name="android:textColor">@color/gray_66</item>    </style>        <style name="DialogButton.Right">        <item name="android:textColor">@color/gray_33</item>    </style>        <style name="LoginEditText">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">54dp</item>        <item name="android:background">@null</item>        <item name="android:gravity">center_vertical</item>        <item name="android:drawablePadding">10dp</item>        <item name="android:textAppearance">@style/Text.F18.Gray33</item>        <item name="android:textColorHint">@color/hint</item>    </style>        <style name="TaskDetailBlock">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginTop">10dp</item>        <item name="android:background">@color/white</item>        <item name="android:paddingLeft">20dp</item>        <item name="android:paddingTop">16dp</item>        <item name="android:paddingRight">20dp</item>        <item name="android:paddingBottom">16dp</item>    </style>        <style name="TaskDetailRow">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginTop">@dimen/task_detail_row_space</item>        <!--<item name="labelWidth">@dimen/task_detail_row_label_width</item>-->    </style>        <style name="MultiTimeFlag">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <!--<item name="android:background">@drawable/bg_flag</item>-->        <item name="android:gravity">center</item>        <item name="android:includeFontPadding">false</item>        <!--<item name="android:text">@string/multi_time</item>-->        <item name="android:textAppearance">@style/Text.F12.Accent</item>    </style>        <style name="SingleRoundedBtn" parent="Text.F18.White">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <!--<item name="android:background">@drawable/btn_single_rounded</item>-->        <item name="android:textStyle">bold</item>    </style>        <style name="PerfectInfoItemLayout">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">55dp</item>        <item name="android:gravity">center_vertical</item>        <item name="android:orientation">horizontal</item>        <item name="android:paddingLeft">16dp</item>        <item name="android:paddingRight">16dp</item>    </style>        <style name="AcceptConditionItem">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:paddingTop">10dp</item>        <item name="android:paddingBottom">10dp</item>    </style>        <style name="AcceptConditionItemLabel">        <item name="android:layout_width">130dp</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_centerVertical">true</item>        <item name="android:textAppearance">@style/Text.F14.Gray66</item>    </style>        <style name="AcceptConditionItemContent">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_alignParentRight">true</item>        <item name="android:layout_centerVertical">true</item>        <item name="android:gravity">right</item>        <item name="android:layout_marginLeft">135dp</item>        <item name="android:textAppearance">@style/Text.F14.Gray66</item>    </style>        <style name="Label">        <item name="android:textAppearance">@style/Text.F14.Gray88</item>    </style>        <style name="TaskItemRow" parent="Label">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:layout_marginLeft">@dimen/horizontal_margin</item>        <item name="android:layout_marginTop">3dp</item>    </style>        <style name="CreateTaskItemLabel">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">#333333</item>        <item name="android:textSize">15dp</item>    </style>        <style name="PerfectInfoItemPhotoLabel">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textColor">#666666</item>        <item name="android:textSize">16dp</item>    </style>        <style name="PerfectInfoItemPhoto">        <item name="android:layout_width">0dp</item>        <item name="android:layout_height">0dp</item>        <!--        <item name="android:background">@drawable/bg_dashed_rect</item>        -->        <item name="android:adjustViewBounds">true</item>        <item name="android:scaleType">fitCenter</item>        <item name="layout_constraintDimensionRatio">H,1.63:1</item>    </style>        <style name="PickPhotoItem">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">44dp</item>        <item name="android:gravity">center</item>        <item name="android:textColor">#333333</item>        <item name="android:textSize">16dp</item>    </style>        <style name="DialogWindowAnim" mce_bogus="1" parent="android:Animation">        <item name="android:windowEnterAnimation">@anim/anim_bottom_in</item>        <item name="android:windowExitAnimation">@anim/anim_bottom_out</item>    </style>        <style name="ReceiptPicView">        <item name="android:layout_width">60dp</item>        <item name="android:layout_height">60dp</item>    </style>        <style name="CustomCheckBoxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">        <!--<item name="android:button">@drawable/checkbox_style</item>-->    </style>        <style name="CreateClueLayout">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">52dp</item>        <item name="android:gravity">center_vertical</item>        <item name="android:layout_marginLeft">@dimen/horizontal_margin</item>        <item name="android:layout_marginRight">@dimen/horizontal_margin</item>        <item name="android:orientation">horizontal</item>    </style>        <style name="CreateClueLabel">        <item name="android:layout_width">wrap_content</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textAppearance">@style/Text.F16.Gray33</item>    </style>        <style name="dialogWindowAnim" mce_bogus="1" parent="android:Animation">        <item name="android:windowEnterAnimation">@anim/anim_bottom_in</item>        <item name="android:windowExitAnimation">@anim/anim_bottom_out</item>    </style>        <style name="FilterItemLayout">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">55dp</item>        <item name="android:gravity">center_vertical</item>        <item name="android:orientation">horizontal</item>        <item name="android:paddingEnd">12dp</item>    </style>        <style name="FilterItemLabel">        <item name="android:layout_width">80dp</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:textAppearance">@style/Text.F14.Gray66</item>    </style>        <style name="EditText">        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>        <item name="android:background">@color/transparent</item>        <item name="android:paddingTop">12dp</item>        <item name="android:paddingBottom">12dp</item>        <item name="android:textAppearance">@style/Text.F16.Gray33</item>        <item name="android:textColorHint">@color/hint</item>    </style>    </resources>